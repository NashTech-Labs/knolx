# This file defines all application routes (Higher priority routes first)
# ~~~~

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                     controllers.Assets.versioned(path="/public", file: Asset)

GET     /webjars/*file                    controllers.WebJarAssets.at(file)

#  home page

GET     /home                             controllers.AuthenticationController.renderHomePage
#  login page
POST    /signin                           controllers.AuthenticationController.signIn
#  logout page
POST    /signout                          controllers.AuthenticationController.signOut
#  signup page
POST     /signup                          controllers.AuthenticationController.signUp
#  dashboard page
GET     /                                 controllers.DashboardController.renderDashBoard





