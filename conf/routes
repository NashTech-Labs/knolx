# This file defines all application routes (Higher priority routes first)
# ~~~~

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file                   controllers.Assets.versioned(path="/public", file: Asset)

GET         /webjars/*file                  controllers.WebJarAssets.at(file)




#  dashboard page
GET         /                               controllers.DashboardController.renderDashBoard
#loginpage
GET         /home                           controllers.AuthenticationController.loginPage
#  sign in
POST        /signin                         controllers.AuthenticationController.signIn
#  logout
GET         /signout                        controllers.AuthenticationController.signOut
#  signup
POST        /signup                         controllers.AuthenticationController.signUp
#get all Users
GET         /getAllUsers                    controllers.UsersController.getAllUsers

GET         /changeStatus/:userId,/:topic    controllers.SessionsController.changeStatus(userId:Long,topic:String)

GET         /deleteSession/:userId     controllers.SessionsController.deleteSession(userId:Long)

#get all Users with status
GET         /getAll                    controllers.SessionsController.getAllUsersWithStatusList

#get admin knolx form
GET         /adminKnolxForm                 controllers.DashboardController.renderKnolxForm

#render Table Page on DashBoard
GET         /tables                         controllers.DashboardController.renderTablePage

POST        /createSession                  controllers.SessionsController.createSession

GET         /updateSession/:userId,/:topic     controllers.SessionsController.updateSession(userId:Long,topic:String)


#javascript routes
GET         /javascriptRoutes               controllers.Application.javascriptRoutes






